{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "plugins": [
      {"name": "typescript-tslint-plugin", "configFile": "./tslint.json"}
    ]
  }
}
